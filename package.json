{
  "name": "dr-cursored",
  "version": "1.0.0",
  "description": "The ultimate debugging and development health toolkit for modern web projects",
  "type": "module",
  "main": "index.js",
  "bin": {
    "dr-cursored": "./bin/dr-cursored.mjs"
  },
  "scripts": {
    "doctor": "node scripts/doctor.mjs",
    "clean": "node scripts/clean.mjs",
    "ports": "node scripts/ports.mjs",
    "services": "node scripts/services.mjs",
    "analyze": "node scripts/analyze.mjs",
    "test": "node scripts/test.mjs",
    "dev": "node scripts/dev.mjs",
    "lint": "eslint . --ext .js,.mjs,.ts",
    "lint:fix": "eslint . --ext .js,.mjs,.ts --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "type-check": "tsc --noEmit",
    "build": "node scripts/build.mjs",
    "prepublishOnly": "npm run lint && npm test && npm run build"
  },
  "keywords": [
    "debugging",
    "development",
    "health-check",
    "monitoring",
    "logging",
    "diagnostics",
    "web-development",
    "cli-tool"
  ],
  "author": "Dr. Cursored Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "glob": "^10.3.10",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "ora": "^7.0.1",
    "inquirer": "^9.2.12"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0",
    "vitest": "^1.0.0",
    "typescript": "^5.3.0"
  },
  "files": [
    "bin/",
    "scripts/",
    "lib/",
    "templates/",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/dr-cursored.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/dr-cursored/issues"
  },
  "homepage": "https://github.com/your-username/dr-cursored#readme"
}
